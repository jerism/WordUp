{
  "settings": {
    "applicationType": "Clicker", // Valid options: Words, Clicker

    "useRealWords": false, // Words -- Use words from dictionary or random jumble
    "displayWords": true, // Words -- Writes words to console window
    "resetConsoleCounter": 0, // Words -- Value of 0 will never clear console

    "showConsole": false, // All -- if false makes the console window as small as possible
    "lockMachineOnCompletion": true, // All -- Automatically locks windows computers after totalRunTimeMins
    "finalRunTimeMins": 180, // All -- additional last time to run application
    "delayMins": 60, // All - delays running of application for specified time
    "beforeDelayMins": 120 // All - runs application prior to the delay
  }
}
